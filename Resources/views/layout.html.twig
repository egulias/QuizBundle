<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <link rel="stylesheet" href="{{ asset('bundles/eguliasquiz/css/bootstrap.min.css') }}" type="text/css" media="all" />
        <script type="text/css">
          body {padding-top: 60px;}
        </script>
        {% block extra_css %}{% endblock %}
        <title>{% block quiz_bundle_title %}Quiz Bundle{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

        {% block extra_js %}{% endblock %}
    </head>
    <body>
        <div id="quiz-wrapper" class="container-fluid">
          {% block navigation %}
          <div class="sidebar">
            <p><a href="{{ path('egulias_quiz_question') }}">Questions List</a></p>
            <p><a href="{{ path('egulias_quiz_question_add') }}">Add Question</a></p>
            <p><a href="{{ path('egulias_quiz_add') }}">Add Quiz</a></p>
            <p><a href="{{ path('egulias_quiz_panel') }}">Quiz List</a></p>
            <p><a href="{{ path('egulias_quiz_answers') }}">Answers List</a></p>
          </div>
          {% endblock %}
          <div class="content">
            {% block quiz_bundle_breadcrumb %}
            {% endblock %}
            {% block content %}{% endblock %}
          </div>
        </div>
    </body>
</html>
